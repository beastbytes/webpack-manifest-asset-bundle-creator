{
    "name": "beastbytes/webpack-manifest-asset-bundle-creator",
    "description": "Creates an asset bundle from a Webpack manifest",
    "keywords": [
        "yii",
        "Webpack",
        "asset bundle",
        "manifest"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Chris Yates",
            "email": "chris.l.yates@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "yiisoft/assets": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "roave/infection-static-analysis-plugin": "^1.0",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.0",
        "yiisoft/event-dispatcher": "^1.0",
        "yiisoft/view": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\Webpack\\Manifest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeastBytes\\Webpack\\Manifest\\Tests\\": "tests"
        }
    },
    "scripts": {
        "infection": "roave-infection-static-analysis-plugin --min-msi=90 --min-covered-msi=90",
        "psalm": "psalm",
        "robo": "robo",
        "test": "phpunit --stop-on-warning --testdox"
    }
}
